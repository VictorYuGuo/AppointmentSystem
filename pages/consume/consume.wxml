<!--tradeAccount.wxml 收入 交易记录-->
<view class="record" wx:for="{{dataList}}" wx:key = "{{id}}" wx:for-index="id">
    <view class="on">
       <text class="action">{{item.reportSubject}}诊疗</text>
       <text class="money">支付：{{item.reportMoney}}</text>
    </view>
  <view class="under">
       <text class="time">交易时间: <text class="detail">{{item.creationTime}}</text> </text>
       <text class="succerr">交易成功</text>
  </view>
</view>
